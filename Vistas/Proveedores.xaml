        <TextBox Height="23" HorizontalAlignment="Left" Margin="244,160,0,0" Name="txtDomicilio" VerticalAlignment="Top" Width="120" />
        <TextBox Height="27" HorizontalAlignment="Left" Margin="244,193,0,0" Name="txtTelefono" VerticalAlignment="Top" Width="120" />
        <Button Content="Salir" Height="23" HorizontalAlignment="Left" Margin="442,215,0,0" Name="btnSalir" VerticalAlignment="Top" Width="75" Click="btnSalir_Click" />
